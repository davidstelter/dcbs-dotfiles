#include ".Xresources.d/base16-chalk.ad"
urxvt*imLocale:en_US.utf8
urxvt*depth:24
!urxvt*background:rgb:0000/0000/0000
!urxvt*foreground:white
urxvt*underlineColor:cyan
urxvt*buffered:false
urxvt*inheritPixmap:false
urxvt*transparent:false
! Shading: 0..100 = darker -1..-100 = lighter
!urxvt*shading:10
! Fading: higher number = darker shade on unfocus
!urxvt*fading:40
! good font setting!
URxvt*font: xft:Terminess Powerline:size=9:style=Regular
URxvt*boldFont: xft:Terminess Powerline:size=9:style=Bold
! good font setting!
! terrible font setting!
!URxvt*font: xft:Sauce Code Powerline:size=10:style=Regular
!URxvt*boldFont: xft:Sauce Code Powerline:size=10:style=Bold
!URxvt*font: xft:Sauce Code Powerline:size=10:style=Regular:antialias=false
!URxvt*boldFont: xft:Sauce Code Powerline:size=10:style=Bold:antialias=false
! terrible font setting!

!urxvt*font:xft:Bitstream Vera Sans Mono:pixelsize=11:autohint=true
!urxvt*boldFont:xft:Bitstream Vera Sans Mono:bold:pixelsize=11:autohint=true
!urxvt*italicFont:xft:Bitstream Vera Sans Mono:bold:pixelsize=11:autohint=true
urxvt*scrollBar:false
!urxvt*scrollStyle:xterm
!urxvt*scrollBar_right:true
!urxvt*scrollBar_floating:true
urxvt*scrollTtyOutput:false
urxvt*scrollTtyKeypress:true
urxvt*scrollWithBuffer:true
urxvt*jumpScroll:false
urxvt*saveLines:6400
urxvt*borderLess:false
urxvt*borderColor:black
!urxvt*internalBorder:4
urxvt*tripleclickwords:true
!urxvt*perl-ext-common:default,matcher,remote-clipboard
urxvt*perl-ext-common: default,clipboard
urxvt*keysym.M-u:perl:matcher:list
urxvt*keysym.Control-Up:\033[1;5A
urxvt*keysym.Control-Down:\033[1;5B
urxvt*keysym.Control-Left:\033[1;5D
urxvt*keysym.Control-Right:\033[1;5C

! trying to unfuck clipboard stuff
urxvt*iso14755: False
!urxvt*keysym.Shift-Control-C: perl:clipboard:copy
!urxvt*keysym.Shift-Control-V: perl:clipboard:paste

!*background:rgb:0000/0000/0000
!*foreground:white
XTerm*locale: true
XTerm*saveLines: 8192
XTerm*reverseVideo: on
!XTerm*faceName: Terminess:style=Regular:size=9
XTerm*faceName: Terminus
!XTerm*faceName: xft:Terminess Powerline
!UXTerm*faceName: xft:Terminess Powerline
XTerm*faceSize: 9

!UXTerm*faceName: Source Code Pro for Powerline
!UXTerm*faceName: Terminus for Powerline
!UXTerm*faceSize: 9
UXTerm*locale: true
UXTerm*saveLines: 8192
UXTerm*reverseVideo: on

!XTerm*faceName: Terminess:style=Regular:size=9
!Xft*dpi: 96
!Xft*antialias: false
!Xft*rgba: rgb
!Xft*hinting: false
!Xft*hintstyle: hintslight
XTerm*VT100.translations: #override <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)

!Rxvt*font: Terminess:style=Regular:size=9
!URxvt*font: xft:Terminess Powerline:size=9
!URxvt*font: xft:Anonymice Powerline:size=9
!URxvt*font: xft:Terminus:size=14

